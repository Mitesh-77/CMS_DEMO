<div class="container text-center ">
    <h3 class="">Welcome To Admin Dashboard</h3>
    <ul class="category_list">
        {{#each headers}}
        <li>
            <a href="">
                <div class="inner_category">
                    <img src="/images/{{file}}" />
                </div>
                <h3>{{title}}</h3>
            </a>
            <form action="/admin/header/edit/{{_id}}" method="get">
                <button class="btn btn-sm btn-warning mr-2 my-2" type="button" contenteditable="false"
                    id="headereditbtn">Edit
                </button>
            </form>
            <form action="/admin/header/delete/{{_id}}?newMethod=DELETE" method="post">
                <button class="btn btn-sm btn-danger" type="submit">Delete</button>
            </form>
        </li>

        {{/each}}
    </ul>

    <form action="/admin/header/create" method="post" enctype="multipart/form-data">

        <div class="form-group">
            <label for="title">Title</label>
            <input type="text" class="form-control" name="title" id="title" placeholder="Enter The Title">
        </div>

        <div class="form-group">
            <label for="file">File Upload</label>
            <input type="file" class="form-control" id="file" name="uploadedFile"
                accept="image/jpeg, image/jpg, image/png, image/bmp">
        </div>
        <button class="btn btn-outline-success btn-lg" type="submit">Create Header Element</button>

    </form>
</div>